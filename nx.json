{
  "npmScope": "todo-application",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "todo": {
      "tags": []
    },
    "todo-e2e": {
      "tags": [],
      "implicitDependencies": ["todo"]
    },
    "todo-data-access": {
      "tags": ["scope:todo", "type:data-access"]
    },
    "todo-feature": {
      "tags": ["scope:todo", "type:feature"]
    }
  }
}
