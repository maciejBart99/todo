{
  "npmScope": "todo-application",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "todo": {
      "tags": ["type:application"]
    },
    "todo-e2e": {
      "tags": ["type:application"],
      "implicitDependencies": ["todo"]
    },
    "todo-regular-feature": {
      "tags": ["scope:todo-regular", "type:feature"]
    },
    "todo-urgent-feature": {
      "tags": ["scope:todo-urgent", "type:feature"]
    },
    "todo-urgent-data-access": {
      "tags": ["scope:todo-urgent", "type:data-access"]
    },
    "todo-regular-data-access": {
      "tags": ["scope:todo-regular", "type:data-access"]
    },
    "todo-domain": {
      "tags": ["scope:todo", "type:domain"]
    },
    "todo-shell": {
      "tags": ["scope:todo", "type:shell"]
    },
    "todo-ui": {
      "tags": ["scope:todo", "type:ui"]
    },
    "auth-data-access": {
      "tags": ["scope:auth", "type:data-access"]
    },
    "auth-public": {
      "tags": ["scope:auth", "type:public"]
    },
    "shared-domain": {
      "tags": ["scope:shared", "type:domain"]
    },
    "shared-ui-user-bar": {
      "tags": ["scope:shared", "type:ui"]
    }
  }
}
