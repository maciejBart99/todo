{
  "npmScope": "todo-application",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "todo": {
      "tags": ["type:application"]
    },
    "todo-e2e": {
      "tags": ["type:application"],
      "implicitDependencies": ["todo"]
    },
    "todo-app-auth-data-access": {
      "tags": ["scope:todo-app-auth", "type:data-access"]
    },
    "todo-app-auth-shell": {
      "tags": ["scope:todo-app-auth", "type:shell"]
    },
    "todo-app-auth-public": {
      "tags": ["scope:todo-app-auth", "type:public"]
    },
    "todo-app-shared-domain": {
      "tags": ["scope:todo-app-shared", "type:domain"]
    },
    "todo-app-shared-ui-header": {
      "tags": ["scope:todo-app-shared", "type:ui"]
    },
    "todo-app-shared-ui-user-bar": {
      "tags": ["scope:todo-app-shared", "type:ui"]
    },
    "todo-app-main-shell": {
      "tags": ["scope:todo-app-main", "type:shell"]
    },
    "todo-app-todo-domain": {
      "tags": ["scope:todo-app-todo", "type:domain"]
    },
    "todo-app-todo-shell": {
      "tags": ["scope:todo-app-todo", "type:shell"]
    },
    "todo-app-todo-ui": {
      "tags": ["scope:todo-app-todo", "type:ui"]
    },
    "todo-app-todo-regular-data-access": {
      "tags": ["scope:todo-app-todo-regular", "type:data-access"]
    },
    "todo-app-todo-urgent-data-access": {
      "tags": ["scope:todo-app-todo-urgent", "type:data-access"]
    },
    "todo-app-todo-urgent-feature": {
      "tags": ["scope:todo-app-todo-urgent", "type:feature"]
    },
    "todo-app-todo-regular-feature": {
      "tags": ["scope:todo-app-todo-regular", "type:feature"]
    },
    "todo-app-core": {
      "tags": ["scope:todo-app", "type:core"]
    }
  }
}
